{
  "name": "@bwaycer/lits",
  "version": "0.0.0",
  "description": "About Lit packages.",
  "type": "module",
  "scripts": {
    "dev": "htRollup server -s ./example/example/src",
    "dev/firstRollup/lit": "NODE_ENV=dev htRollup build asset -s ./firstRollupSrc/lit -t ./example/example/src -f",
    "dev/build": "NODE_ENV=dev htRollup build -s ./example/example/src"
  },
  "exports": {
    ".": "./src/lit.js",
    "./mod/lit": "./src/mod/lit.js",
    "./useLitPkg": "./src/useLitPkg.js"
  },
  "peerDependencies": {
    "lit": ">=3.0.0-pre.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.4"
  }
}
