doctype html
html
  head
    meta(name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1")
    title
      if title
        | #{title}
      else
        | Template
    script(defer type="module" src=`${scriptUrl}`)
    if styleUrl
      link(
        rel="stylesheet"
        href=`${styleUrl}`
        media="none"
        onload="media='all'"
      )
  body
    #view
      .
        !{content}

